{
  "extends": ["airbnb-typescript/base", "prettier", "prettier/@typescript-eslint", "prettier/prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "ignorePatterns": ["dist", "lib"],
  "rules": {
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off"
  }
}
